{
	"plugins": ["prettier"],
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
		"no-unused-vars": 0,
        "prettier/prettier": [
			"error",
			{
				"endOfLine": "auto"
			}
		]
	}
}